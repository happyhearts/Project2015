#include<iostream>
using namespace std;
int main(){
	int noOfPeople;
	float probability;
	cout<<"enter number of people\n";
	cin>>noOfPeople;
	int k = noOfPeople;
	int i=0;
	float x=1;
	for(;i<k;i++){
		x *= (365 - i)/365.0;
		cout<<x<<endl;
	}
	probability = (1 - x)*100;
	cout<<"Probability = "<<probability<<endl;	
	
}
